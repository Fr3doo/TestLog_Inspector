{
  "name": "testlog-inspector",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tests"
  ],
  "scripts": {
    "dev": "turbo run build --filter @testlog-inspector/log-parser && turbo run dev --parallel",
    "dev:all": "turbo run build --filter @testlog-inspector/log-parser && turbo run dev --parallel",
    "build": "turbo run build",
    "start": "turbo run start --parallel",
    "start:prod": "concurrently -n api,web -c green,blue \"pnpm --filter @testlog-inspector/api start:prod\" \"pnpm --filter @testlog-inspector/web start\"",
    "lint": "turbo run lint",
    "format": "prettier \"**/*.{ts,tsx,js,json,md,yml,yaml,cjs,mjs,css}\"",
    "format:write": "pnpm format --write",
    "check": "pnpm lint && pnpm test && git ls-files -m | grep -E '\\.(ts|tsx|js|json|md|yml|yaml|cjs|mjs|css)$' | xargs -r pnpm exec prettier --check",
    "test": "turbo run test",
    "ci": "turbo run lint build test --no-cache",
    "precommit": "lint-staged",
    "help": "node scripts/help.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-n": "^17.21.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,md}": "prettier --write"
  },
  "overrides": {
    "multer": "^2.0.2",
    "esbuild": "^0.25.0"
  },
  "pnpm": {
    "overrides": {
      "glob": "^10.3.10"
    }
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "jspdf-autotable": "^5.0.2"
  }
}

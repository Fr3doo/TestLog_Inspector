{
  "name": "@testlog-inspector/api",
  "version": "0.0.1",
  "private": true,
  "description": "NestJS API for TestLog Inspector (log upload & analysis).",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "dev": "nest start --watch",
    "build": "nest build",

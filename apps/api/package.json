    "reflect-metadata": "^0.1.13",
    "zod": "^3.23.0",
    "@testlog-inspector/log-parser": "workspace:*"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.1",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"

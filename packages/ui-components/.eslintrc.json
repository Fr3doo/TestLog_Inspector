{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {},
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.cts", "*.mts"],
      "rules": {
        "no-undef": "off"
      }
    }
  ]
}
